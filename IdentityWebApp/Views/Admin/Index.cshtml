@model List<AppUser>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Admin Index</h3>

<table class="table table-bordered table-striped">
    <tr>
        <td>USer Id</td>
        <td>USer Name</td>
        <td>Email</td>
    </tr>
    @if (Model.Count() <= 0)
    {
        <tr>
            <td colspan="3">Hiç bir üye yoktur.</td>
        </tr>
    }
    else
    {
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }
    }
</table>